{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

    "SecurityHeaders": {
      "ContentSecurityPolicy": "default-src 'self'; script-src 'self' 'unsafe-inline'",
      "XContentTypeOptions": "nosniff",
      "XFrameOptions": "DENY",
      "XXssProtection": "1; mode=block"
    }
 }